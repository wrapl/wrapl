if WINDOWS then
	_export = export
	export = function(Name)
		_export("_" .. Name, Name)
	end
	_subexport = subexport
	subexport = function(Module, Internal, External)
		_subexport(Module, "_" .. Internal, External)
	end
end

export("Collect")
export("Alloc")
export("AllocAtomic")
export("AllocUncollectable")
export("RegisterFinalizer")
export("RegisterObjectFinalizer")
export("IsVisible")
export("GCEnabled")

submodule("WeakRef")
subexport("WeakRef", "WeakRefNew", "New")
subexport("WeakRef", "WeakRefGet", "Get")


