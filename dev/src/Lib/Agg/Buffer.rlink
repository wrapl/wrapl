export("T")
export("Compare")
export("Hash")
export("Nil")
export("New")
export("NewAtomic")
export("_alloc")
export("_new")

function typed_buffer(Name)
	submodule(Name)
	subexport(Name, Name .. "T", "T")
	subexport(Name, Name .. "New", "New")
	subexport(Name, Name .. "_new", "_new")
end

typed_buffer("Int8")
typed_buffer("Int16")
typed_buffer("Int32")
typed_buffer("Float32")
typed_buffer("Float64")